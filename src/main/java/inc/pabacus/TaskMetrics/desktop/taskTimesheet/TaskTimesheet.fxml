<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXDatePicker?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane fx:id="mainPane" prefHeight="720.0" prefWidth="925.0" xmlns="http://javafx.com/javafx/8.0.172-ea"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="inc.pabacus.TaskMetrics.desktop.taskTimesheet.TaskTimesheetPresenter">
  <BorderPane layoutX="30.0" layoutY="30.0" prefHeight="720.0" prefWidth="925.0" AnchorPane.bottomAnchor="0.0"
              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
    <top>
      <AnchorPane prefHeight="53.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <JFXDatePicker fx:id="datePicker" layoutX="25.0" layoutY="11.0" onAction="#filterSummaries"/>
      </AnchorPane>
    </top>
    <center>
      <TableView fx:id="taskTimesheet" prefHeight="493.0" prefWidth="875.0"
                 style="-fx-background-color: transparent; -fx-border-color: lightgray;" BorderPane.alignment="CENTER">
        <columnResizePolicy>
          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
        <BorderPane.margin>
          <Insets bottom="25.0" left="25.0" right="25.0"/>
        </BorderPane.margin>
      </TableView>
    </center>
    <bottom>
      <BorderPane visible="false" BorderPane.alignment="CENTER">
        <bottom>
          <BorderPane BorderPane.alignment="CENTER">
            <top>
              <GridPane alignment="TOP_CENTER" BorderPane.alignment="CENTER">
                <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                  <ColumnConstraints minWidth="10.0" prefWidth="100.0"/>
                  <ColumnConstraints minWidth="10.0" prefWidth="100.0"/>
                  <ColumnConstraints minWidth="10.0" prefWidth="100.0"/>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                </columnConstraints>
                <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                </rowConstraints>
                <children>
                  <Label text="Total">
                    <font>
                      <Font name="System Bold" size="15.0"/>
                    </font>
                  </Label>
                  <Label text="Billable:" GridPane.columnIndex="1">
                    <font>
                      <Font name="System Italic" size="15.0"/>
                    </font>
                  </Label>
                  <Label fx:id="totalbillable" text="00.00" GridPane.columnIndex="2"/>
                  <Label text="Non-Billable:" GridPane.columnIndex="3">
                    <font>
                      <Font name="System Italic" size="15.0"/>
                    </font>
                  </Label>
                  <Label fx:id="totalNonBillable" text="00.00" GridPane.columnIndex="4"/>
                  <Label text="Total Hours:" GridPane.columnIndex="5">
                    <font>
                      <Font name="System Italic" size="15.0"/>
                    </font>
                  </Label>
                  <Label fx:id="totalHours" text="00.00" GridPane.columnIndex="6"/>
                  <Label text="\% Billable:" GridPane.columnIndex="7">
                    <font>
                      <Font name="System Italic" size="15.0"/>
                    </font>
                  </Label>
                  <Label fx:id="totalPercentBillable" text="00.00" GridPane.columnIndex="8"/>
                  <Label text="Invoice Amount:" GridPane.columnIndex="9">
                    <font>
                      <Font name="System Italic" size="15.0"/>
                    </font>
                  </Label>
                  <Label fx:id="totalInvoice" text="00.00" GridPane.columnIndex="10"/>
                </children>
              </GridPane>
            </top>
            <BorderPane.margin>
              <Insets bottom="25.0" left="25.0" right="25.0"/>
            </BorderPane.margin>
          </BorderPane>
        </bottom>
      </BorderPane>
    </bottom>
  </BorderPane>
</AnchorPane>
