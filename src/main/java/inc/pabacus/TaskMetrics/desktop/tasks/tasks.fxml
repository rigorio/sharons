<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="720" prefWidth="591.0" xmlns="http://javafx.com/javafx/8.0.172-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="inc.pabacus.TaskMetrics.desktop.tasks.TasksPresenter">
  <children>
    <Label fx:id="timerLabelFx" layoutX="29.0" layoutY="90.0" prefHeight="31.0" prefWidth="47.0" text="Timer:"/>
    <Label fx:id="timerLabel" layoutX="76.0" layoutY="90.0" prefHeight="31.0" prefWidth="86.0"/>
    <JFXButton alignment="CENTER" buttonType="RAISED" layoutX="144.0" layoutY="11.0" onAction="#viewScreenShot"
               prefHeight="35.0" prefWidth="108.0" style="-fx-background-color: #008080;" text="View Screen Shot"
               textFill="WHITE"/>
    <Label ellipsisString="" layoutY="-105.0" prefHeight="131.0" prefWidth="844.0"
           text="_________________________________________________________________" textFill="#008080"
           textOverrun="CLIP">
      <font>
        <Font name="System Bold" size="85.0"/>
      </font>
    </Label>
    <JFXButton alignment="CENTER" buttonType="RAISED" layoutX="12.0" layoutY="11.0" onAction="#newTask"
               prefHeight="31.0" prefWidth="108.0" style="-fx-padding: 0,0,10,0; -fx-background-color: #008080;"
               text="+ New Task" textFill="WHITE"/>
    <TableView fx:id="tasksTable" layoutX="29.0" layoutY="134.0" prefHeight="172.0" prefWidth="787.0">
      <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
      </columnResizePolicy>
    </TableView>
    <Label fx:id="taskName" layoutX="29.0" layoutY="64.0"/>
    <JFXButton fx:id="startButton" layoutX="274.0" layoutY="90.0" mnemonicParsing="false" onAction="#startTask"
               text="Start"/>
    <JFXButton fx:id="completeButton" layoutX="367.0" layoutY="90.0" mnemonicParsing="false" onAction="#completeTask"
               text="Complete"/>
    <TableView fx:id="taskTimesheet" layoutX="29.0" layoutY="318.0" prefHeight="172.0" prefWidth="787.0">
      <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
      </columnResizePolicy>
    </TableView>
    <JFXTextField fx:id="titleText" layoutX="29.0" layoutY="512.0"/>
    <JFXComboBox fx:id="progressText" layoutX="29.0" layoutY="621.0" prefHeight="31.0" prefWidth="169.0"/>
    <JFXComboBox fx:id="statusText" layoutX="29.0" layoutY="567.0" prefHeight="31.0" prefWidth="169.0"/>
    <JFXComboBox fx:id="priorityText" layoutX="29.0" layoutY="668.0" prefHeight="31.0" prefWidth="169.0"/>
    <JFXTextArea fx:id="descriptionText" layoutX="252.0" layoutY="512.0" prefHeight="109.0" prefWidth="563.0"/>
    <JFXButton fx:id="deleteTask" onAction="#deleteThis" layoutX="674.0" layoutY="653.0" mnemonicParsing="false"
               style="-fx-background-color: #ef5350;" text="Delete" textFill="WHITE"/>
    <JFXButton fx:id="saveTask" onAction="#saveThis" layoutX="750.0" layoutY="653.0" mnemonicParsing="false"
               style="-fx-background-color: #008080;" text="Save" textFill="WHITE"/>
  </children>

</AnchorPane>
