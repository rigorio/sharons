<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="mainPane" prefHeight="720" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.172-ea"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="inc.pabacus.TaskMetrics.desktop.timesheet.TimesheetPresenter">
  <TableView id="timesheet" fx:id="timeSheet" layoutX="14.0" layoutY="303.0" prefHeight="403.0" prefWidth="872.0"/>
  <JFXComboBox fx:id="comboBox" layoutX="30.0" layoutY="112.0" onAction="#updateStatus" prefHeight="31.0"
               prefWidth="179.0"/>
  <JFXButton fx:id="updateButton" layoutX="241.0" layoutY="217.0" mnemonicParsing="false" ripplerFill="#64d8cb"
             style="-fx-background-color: #008080;" text="Update" textFill="WHITE"/>
  <JFXButton fx:id="statusButton" layoutX="224.0" layoutY="119.0" mnemonicParsing="false" onAction="#changeStatus"
             ripplerFill="#64d8cb" style="-fx-background-color: #008080;" text="Change Status" textFill="WHITE"/>
  <Label layoutX="61.0" layoutY="81.0" text="You are currently:"/>
  <Label fx:id="statusText" layoutX="186.0" layoutY="81.0" text="Label" textFill="#008080">
    <font>
      <Font name="System Bold" size="15.0"/>
    </font>
  </Label>
  <Label layoutX="61.0" layoutY="31.0" text="Hi, ">
    <font>
      <Font size="30.0"/>
    </font>
  </Label>
  <Label fx:id="userName" layoutX="105.0" layoutY="31.0" text="Rigo">
    <font>
      <Font size="30.0"/>
    </font>
  </Label>
  <Label fx:id="dateLabel" layoutX="767.0" layoutY="247.0" text="date" visible="false"/>
</AnchorPane>
