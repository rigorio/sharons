<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXDatePicker?>
<?import com.jfoenix.controls.JFXTextArea?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox fx:id="mainPane" prefHeight="448.0" prefWidth="982.0" xmlns="http://javafx.com/javafx/8.0.999-ea"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="rigor.io.Sharons.dashboard.DashboardPresenter">
  <VBox prefHeight="521.0" prefWidth="285.0">
    <AnchorPane prefHeight="520.0" prefWidth="285.0">
      <BorderPane prefHeight="521.0" prefWidth="285.0">
        <center>
          <AnchorPane prefHeight="259.0" prefWidth="285.0" BorderPane.alignment="CENTER">
            <JFXTextField fx:id="nameText" layoutX="31.0" layoutY="14.0" prefHeight="31.0" prefWidth="220.0"
                          promptText="Name"/>
            <JFXTextArea fx:id="descText" layoutX="31.0" layoutY="51.0" prefHeight="75.0" prefWidth="220.0"
                         promptText="Description"/>
            <JFXTextField fx:id="priceText" layoutX="31.0" layoutY="127.0" prefHeight="31.0" prefWidth="220.0"
                          promptText="Price"/>
            <JFXComboBox fx:id="statusBox" layoutX="31.0" layoutY="169.0" prefHeight="31.0" prefWidth="220.0"
                         promptText="Select status"/>
            <JFXDatePicker fx:id="dateRentedText" layoutX="31.0" layoutY="233.0" prefHeight="25.0"
                           prefWidth="220.0"/>
            <JFXDatePicker fx:id="dueDateText" layoutX="31.0" layoutY="295.0" prefHeight="25.0" prefWidth="220.0"/>
            <JFXTextField fx:id="clientText" layoutX="31.0" layoutY="342.0" prefWidth="220.0" promptText="Client"/>
            <JFXTextField fx:id="contactText" layoutX="31.0" layoutY="393.0" prefWidth="220.0"
                          promptText="Contact"/>
            <JFXButton fx:id="updateButton" layoutX="51.0" layoutY="443.0" mnemonicParsing="false" onAction="#add"
                       prefHeight="25.0" prefWidth="181.0" text="Add Item"/>
            <Label layoutX="31.0" layoutY="216.0" text="Date Rented"/>
            <Label layoutX="31.0" layoutY="278.0" text="Due Date"/>
          </AnchorPane>
        </center>
      </BorderPane>
    </AnchorPane>
  </VBox>
  <BorderPane prefHeight="521.0" prefWidth="701.0">
    <center>
      <TableView fx:id="gownsTable" prefHeight="410.0" prefWidth="697.0"
                 style="-fx-background-color: transparent; -fx-border-color: lightgray;"
                 BorderPane.alignment="CENTER">
        <columnResizePolicy>
          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
      </TableView>
    </center>
    <top>
      <AnchorPane prefHeight="53.0" prefWidth="697.0" BorderPane.alignment="CENTER">
        <Hyperlink onAction="#delete" text="Delete"/>
        <Hyperlink layoutX="71.0" layoutY="28.0" onAction="#refresh" text="Refresh"/>
        <JFXTextField fx:id="filterText" layoutX="156.0" layoutY="8.0" onKeyReleased="#filter" prefHeight="31.0"
                      prefWidth="142.0" promptText="Search"/>
        <JFXComboBox fx:id="statusSearchText" layoutX="388.0" layoutY="11.0" onAction="#filter" prefHeight="31.0"
                     prefWidth="154.0" promptText="Status"/>
      </AnchorPane>
    </top>
  </BorderPane>
</HBox>
