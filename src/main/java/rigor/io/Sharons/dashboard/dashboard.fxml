<?import javafx.scene.layout.*?>
<AnchorPane fx:id="mainPane" prefHeight="720" prefWidth="925" xmlns="http://javafx.com/javafx/8.0.172-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="rigor.io.Sharons.dashboard.DashboardPresenter">
  <BorderPane layoutY="73.0" prefHeight="647.0" prefWidth="925.0" AnchorPane.bottomAnchor="0.0"
              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="73.0">
    <center>
      <TableView fx:id="gownsTable" prefHeight="695.0" prefWidth="875.0"
                 style="-fx-background-color: transparent; -fx-border-color: lightgray;" BorderPane.alignment="CENTER">
        <columnResizePolicy>
          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
        <BorderPane.margin>
          <Insets bottom="25.0" left="25.0" right="25.0"/>
        </BorderPane.margin>
      </TableView>
    </center>
  </BorderPane>
  <JFXTextField fx:id="filterText" layoutX="35.0" layoutY="29.0" onKeyReleased="#filter" prefHeight="31.0"
                prefWidth="142.0" promptText="Search"/>
  <JFXTextField fx:id="priceText" layoutX="344.0" layoutY="29.0" onKeyReleased="#priceFilter"/>
  <JFXComboBox fx:id="priceBox" layoutX="190.0" layoutY="29.0" onAction="#priceFilter" prefHeight="31.0"
               prefWidth="154.0"/>
</AnchorPane>
