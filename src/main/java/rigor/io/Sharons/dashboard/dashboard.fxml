<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="mainPane" prefHeight="720" prefWidth="925" xmlns="http://javafx.com/javafx/8.0.172-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="rigor.io.Sharons.dashboard.DashboardPresenter">
  <BorderPane prefHeight="720.0" prefWidth="925.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
    <center>
      <TableView fx:id="gownsTable" prefHeight="658.0" prefWidth="890.0"
                 style="-fx-background-color: transparent; -fx-border-color: lightgray;" BorderPane.alignment="CENTER">
        <columnResizePolicy>
          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
        <BorderPane.margin>
          <Insets bottom="25.0" left="25.0" right="25.0"/>
        </BorderPane.margin>
      </TableView>
    </center>
    <top>
      <AnchorPane prefHeight="43.0" prefWidth="992.0" BorderPane.alignment="CENTER">
        <BorderPane prefHeight="70.0" prefWidth="992.0">
          <top>
            <GridPane prefHeight="44.0" prefWidth="992.0" BorderPane.alignment="CENTER">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
              </rowConstraints>
              <BorderPane prefHeight="29.0" prefWidth="511.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                <right>
                  <HBox prefHeight="100.0" prefWidth="216.0" BorderPane.alignment="CENTER">
                    <Hyperlink onAction="#add" text="Add"/>
                    <Hyperlink onAction="#edit" text="Edit"/>
                    <Hyperlink onAction="#delete" text="Delete"/>
                    <Hyperlink onAction="#refresh" text="Refresh"/>
                  </HBox>
                </right>
              </BorderPane>
              <BorderPane prefHeight="61.0" prefWidth="496.0" GridPane.rowIndex="1">
                <center>
                  <HBox prefHeight="31.0" prefWidth="410.0" BorderPane.alignment="CENTER">
                    <JFXTextField fx:id="filterText" onKeyReleased="#filter" prefHeight="31.0" prefWidth="142.0"
                                  promptText="Search"/>
                    <JFXComboBox fx:id="priceBox" onAction="#filter" prefHeight="31.0" prefWidth="154.0"/>
                    <JFXTextField fx:id="priceText" onKeyReleased="#filter" prefHeight="31.0" prefWidth="79.0"
                                  promptText="Price"/>
                  </HBox>
                </center>
              </BorderPane>
            </GridPane>
          </top>
        </BorderPane>
      </AnchorPane>
    </top>
  </BorderPane>
