<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="mainPane" prefHeight="720" prefWidth="925" style="-fx-background-color: fffff;"
            xmlns="http://javafx.com/javafx/8.0.999-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="rigor.io.Sharons.dashboard.DashboardPresenter">
  <AnchorPane layoutX="279.0" prefHeight="720.0" prefWidth="697.0">
    <TableView fx:id="gownsTable" layoutY="47.0" prefHeight="673.0" prefWidth="697.0"
               style="-fx-background-color: transparent; -fx-border-color: lightgray;">
      <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
      </columnResizePolicy>
    </TableView>
    <Hyperlink layoutX="640.0" layoutY="9.0" onAction="#delete" text="Delete"/>
    <Hyperlink layoutX="592.0" layoutY="9.0" onAction="#refresh" text="Refresh"/>
    <JFXTextField fx:id="filterText" layoutX="14.0" layoutY="5.0" onKeyReleased="#filter" prefHeight="31.0"
                  prefWidth="142.0" promptText="Search"/>
    <JFXComboBox fx:id="priceOptionsText" layoutX="342.0" layoutY="5.0" onAction="#filter" prefHeight="31.0"
                 prefWidth="154.0"/>
    <JFXTextField fx:id="priceSearchText" layoutX="513.0" layoutY="5.0" onKeyReleased="#filter" prefHeight="31.0"
                  prefWidth="79.0" promptText="Price"/>
    <JFXComboBox fx:id="statusSearchText" layoutX="174.0" layoutY="5.0" onAction="#filter" prefHeight="31.0"
                 prefWidth="154.0" promptText="Status"/>
  </AnchorPane>
  <VBox layoutX="-1.0" prefHeight="720.0" prefWidth="248.0">
    <AnchorPane prefHeight="520.0" prefWidth="285.0">
      <BorderPane prefHeight="521.0" prefWidth="285.0">
        <center>
          <AnchorPane prefHeight="259.0" prefWidth="285.0" BorderPane.alignment="CENTER">
            <JFXTextField fx:id="nameText" layoutX="31.0" layoutY="14.0" prefHeight="31.0" prefWidth="220.0"
                          promptText="Name"/>
            <JFXTextField fx:id="descText" layoutX="31.0" layoutY="72.0" prefHeight="31.0" prefWidth="220.0"
                          promptText="Description"/>
            <JFXTextField fx:id="priceText" layoutX="31.0" layoutY="127.0" prefHeight="31.0" prefWidth="220.0"
                          promptText="Price"/>
            <JFXComboBox fx:id="statusBox" layoutX="31.0" layoutY="169.0" prefHeight="31.0" prefWidth="220.0"
                         promptText="Select status"/>
            <JFXButton layoutX="51.0" layoutY="449.0" mnemonicParsing="false" onAction="#add" prefHeight="25.0"
                       prefWidth="181.0" text="Add Item"/>
            <JFXDatePicker fx:id="dateRentedText" layoutX="31.0" layoutY="226.0" prefHeight="25.0" prefWidth="220.0"/>
            <JFXDatePicker fx:id="dueDateText" layoutX="31.0" layoutY="288.0" prefHeight="25.0" prefWidth="220.0"/>
            <Label layoutX="31.0" layoutY="209.0" text="Date Rented"/>
            <Label layoutX="31.0" layoutY="271.0" text="Due Date"/>
            <Separator layoutX="279.0" orientation="VERTICAL" prefHeight="243.0" prefWidth="0.0"/>
            <Separator layoutX="41.0" layoutY="481.0" prefWidth="200.0"/>
          </AnchorPane>
        </center>
      </BorderPane>
    </AnchorPane>
  </VBox>
</AnchorPane>
